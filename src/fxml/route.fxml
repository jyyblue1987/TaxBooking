<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" styleClass="background" stylesheets="css/app.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.routeController">
   <children>
      <Button fx:id="bus" layoutY="2.0" mnemonicParsing="false" onAction="#Routing" prefHeight="84.0" prefWidth="163.0" text="Bus" />
      <Button fx:id="admin" layoutX="163.0" layoutY="2.0" mnemonicParsing="false" onAction="#Routing" prefHeight="84.0" prefWidth="163.0" text="Customers" />


      <Button fx:id="settings" layoutX="954.0" layoutY="2.0" mnemonicParsing="false" onAction="#Routing" prefHeight="84.0" prefWidth="163.0" text="Settings" />
      <Button fx:id="logout" layoutX="1117.0" layoutY="2.0" mnemonicParsing="false" onAction="#Routing" prefHeight="84.0" prefWidth="163.0" text="Log Out" />
      
      <Label fx:id="title" layoutX="455.0" layoutY="90.0" minHeight="16" minWidth="69" text="Booking History" />
      
      <TextField fx:id="search" layoutX="860.0" layoutY="90.0" prefHeight="32.0" prefWidth="337.0" promptText="Search Booking History" />


      <TableView fx:id="table" layoutX="33.0" layoutY="155.0" prefHeight="663.0" prefWidth="1199.0">
         <columns>
            <TableColumn fx:id="name" minWidth="0.0" prefWidth="139.0" text="Name" />
            <TableColumn fx:id="phone" prefWidth="129.0" text="Phone" />
            <TableColumn fx:id="email" minWidth="0.0" prefWidth="139.0" text="Email" />
            <TableColumn fx:id="bus_name" prefWidth="75.0" text="Bus Name" />
            <TableColumn fx:id="bus_id" prefWidth="75.0" text="Bus ID" />
            <TableColumn fx:id="capacity" prefWidth="75.0" text="Capacity" />
            <TableColumn fx:id="departure" prefWidth="124.0" text="Departure" />
            <TableColumn fx:id="return1" prefWidth="75.0" text="Return" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      
   </children>
</AnchorPane>
